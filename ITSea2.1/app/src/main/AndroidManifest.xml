<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.itsea">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icons8dragonboat"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icons8dragonboat"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.data_ITSea.Data201111Activity" />
        <activity android:name=".JsonActivity" />
        <activity
            android:name=".ui.login.JsonActivity"
            android:label="@string/title_activity_json" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".RealtimeActivity" />
        <activity android:name=".AlarmActivity" />
        <activity android:name=".SensorActivity" />
        <activity android:name=".DatabaseActivity" />
    </application>

</manifest>